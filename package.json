{
  "name": "vue-prevent-back",
  "version": "0.0.5",
  "title": "Vue Prevent Back",
  "description": "prevent back directive for Vue.js.",
  "author": "Changwan Jun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/vue-prevent-back.git"
  },
  "scripts": {
    "dev": "poi example-src/entry.js --config build/poi.config.js",
    "build:production": "rimraf dist/* && cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.prod.conf.js",
    "build:example": "rimraf example/* && cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.example.conf.js",
    "build": "npm run build:production && npm run build:example"
  },
  "main": "dist/index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.0",
    "poi": "^9.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^5.0.1",
    "uglifyjs-webpack-plugin": "^1",
    "vue": "^2.4.4",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
